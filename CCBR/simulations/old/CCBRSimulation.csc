      <skin>se.sics.cooja.plugins.skins.UDGMVisualizerSkin</skin>
      <skin>se.sics.cooja.plugins.skins.GridVisualizerSkin</skin>
      <viewport>3.5367513456002664 0.0 0.0 3.5367513456002664 90.79170245797403 -116.98570293300523</viewport>
    </plugin_config>
    <width>400</width>
    <z>3</z>
    <height>440</height>
    <location_x>1</location_x>
    <location_y>1</location_y>
  </plugin>
  <plugin>
    se.sics.cooja.plugins.LogListener
    <plugin_config>
      <filter />
      <formatted_time />
      <coloring />
